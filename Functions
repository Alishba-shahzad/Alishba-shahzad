Question 2
type Product ={
id:number
name:string
price:number
}
const product = Product[] = [
{id :1 , name:"Ali", price:20000}
{id :2 , name:"sam", price:5000}
]
const productName = product.map(p => p.name);

Next transform

type Product = {
label : string
price :number 
}

const productDetail : Product[] = products.map(p=>({
label:p.name,
price:p.price,
}))
